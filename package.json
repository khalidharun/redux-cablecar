{
  "name": "redux-cablecar",
  "version": "1.2.0",
  "description": "Redux middleware for Rails ActionCable",
  "main": "./dist/bundle.js",
  "scripts": {
    "build:umd": "webpack src/index.js dist/bundle.js",
    "build:umd:min": "webpack -p src/index.js dist/bundle.min.js",
    "build": "npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run build",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-core/register"
  },
  "author": "Khalid Harun",
  "repository": {
    "type": "git",
    "url": "https://github.com/khalidharun/redux-cablecar"
  },
  "keywords": [
    "cablecar",
    "actioncable",
    "rails",
    "rails5",
    "redux",
    "redux-cablecar"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "actioncable": "^5.1.0",
    "redux": "^3.6.0"
  }
}
